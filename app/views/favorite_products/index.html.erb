<h1 class="center"> Your Wishlist <h1>
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Product Name</th>
        <th>Product Price</th>
        <th>Product status</th>
        <th colspan="3"></th>

      </tr>
    </thead>

    <tbody>
      <% @favorites.each do |favorite| %>
        <tr>

          <td><%= favorite.productname%></td>
          <td><%= favorite.productprice%></td>
          <% if favorite.productstatus == true %>
          <td>Available</td>
          <% elsif  %>
          <td>Not Available </td>
          <% end %>







        </tr>
      <% end %>
    </tbody>
  </table>
